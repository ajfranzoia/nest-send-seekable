{
  "name": "nest-send-seekable",
  "version": "0.0.1",
  "description": "NestJs implementation for send-seekable",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "send-seekable"
  ],
  "author": "Augusto Franzoia",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajfranzoia/nest-send-seekable.git"
  },
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --runInBand --watch",
    "test:cov": "jest --runInBand --coverage",
    "format": "prettier --write .",
    "lint": "eslint {src/**/*.ts,test/**/*.ts} --fix",
    "prepublish:npm": "npm run build",
    "prerelease": "npm run build",
    "release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "is-stream": "^2.0.1",
    "range-parser": "^1.2.1",
    "range-stream": "^2.0.0",
    "simple-bufferstream": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@latipun7/commitlintrc": "1.1.3",
    "@nestjs/common": "9.3.10",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/testing": "^9.3.10",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/range-parser": "^1.2.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "^2.8.4",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.0",
    "rxjs": "7.8.0",
    "semantic-release": "^20.1.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 6.10.0 < 10",
    "@nestjs/platform-express": "^6.10.10 < 10",
    "reflect-metadata": "^0.1.12",
    "rxjs": ">= 6.0.0 < 8"
  },
  "commitlint": {
    "extends": [
      "@latipun7/commitlintrc"
    ]
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*": "prettier --write --ignore-unknown"
  }
}
